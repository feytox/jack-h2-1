class Player {
    field int _gameX;
    field int _gameY;

    constructor Player new(int gameX, int gameY) {
        let _gameX = gameX;
        let _gameY = gameY;
        return this;
    }

    method void render(int mapStartX, int mapStartY) {
        // TODO: player render
        do Screen.drawCircle(mapStartX + (_gameX * 16) + 8, mapStartY + (_gameY * 16) + 8, 4);
        return;
    }

    method int getX() { return _gameX; }
    method int getY() { return _gameY; }
}